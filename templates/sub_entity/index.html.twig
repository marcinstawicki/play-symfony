{% extends 'base.html.twig' %}

{% block title %}SubEntity index{% endblock %}

{% block body %}
    <h1>SubEntity index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>DoctrineStringPhpStringFormText1</th>
                <th>DoctrineStringPhpStringFormText2</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sub_entity in sub_entities %}
            <tr>
                <td>{{ sub_entity.id }}</td>
                <td>{{ sub_entity.doctrineStringPhpStringFormText1 }}</td>
                <td>{{ sub_entity.doctrineStringPhpStringFormText2 }}</td>
                <td>
                    <a href="{{ path('sub_entity_show', {'id': sub_entity.id}) }}">show</a>
                    <a href="{{ path('sub_entity_edit', {'id': sub_entity.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('sub_entity_new') }}">Create new</a>
{% endblock %}
